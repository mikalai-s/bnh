<?xml version="1.0"?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
    <system.diagnostics>
        <switches>
            <add name="Trace" value="All"/>
        </switches>
        <trace autoflush="true" indentsize="4" />
        <sources>
            <source name="Trace" switchName="Trace" />
        </sources>
    </system.diagnostics>
</configuration>
