@model BrickContent

@{
    string suffix =
        (Model is RazorContent) ? "Razor" :
        (Model is HtmlContent) ? "Html" :
        (Model is MapContent) ? "Map" :
        (Model is TocContent) ? "Toc" :
        null;
}
@if (!suffix.IsEmpty())
{ 
    @Html.Partial("~/WebExtracted/Ms.Cms/Views/Brick/BrickContent_" + suffix + ".cshtml", Model);
}