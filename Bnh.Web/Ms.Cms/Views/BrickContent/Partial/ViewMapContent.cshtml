@model MapContent
@{
    this.GetScriptBundle().Include(new []
    {
        Ms.Cms.MsCms.GoogleMapsApiScript,
        "~/Ms.Cms/Scripts/Map.js",
        "~/Ms.Cms/Scripts/BrickContent_Map.js"
    });
}
@*TODO: remove community dependency from here*@
@{
    var community = ViewBag.GlobalModel as Bnh.Entities.Community;
}
@if (community != null)
{ 
<div class="map-canvas-wrapper"  zoom="@Model.Zoom" gpsLocation="@community.GpsLocation" gpsBounds="@community.GpsBounds">
    <div id="mapCanvas" style="height: @(Model.Height)px"></div>
</div>
}
else
{
    @:No community entity available
}