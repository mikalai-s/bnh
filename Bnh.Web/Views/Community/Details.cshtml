@model Community
@{
    ViewBag.Title = Model.Name;
    ViewBag.Description = Model.Name;
    ViewBag.GlobalModel = Model;
}

<div class="scene">
    <h5>
        @if (Context.User.IsInRole("content_manager"))
        {
            @Html.ActionLink("edit properties", "Edit", new { id = Model.UrlId }, null)<br />
            @Html.ActionLink("edit scene", "EditScene", new { id = Model.UrlId }, null)
        }
    </h5>
    @{ this.RenderScene(Model.CommunityId, Model); }
</div>